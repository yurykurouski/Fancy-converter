{
  "name": "fancy-converter",
  "version": "1.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug",
    "ios": "react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "start": "react-native start",
    "start:android": "adb reverse tcp:8081 tcp:8081 && react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type:check": "tsc --noemit --skipLibCheck",
    "upgrade": "yarn upgrade-interactive",
    "nuke": "rm -rf node_modules; rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock; yarn; yarn podinstall",
    "findDead": "ts-prune | grep -v '(used in module)'",
    "pods": "cd ios; pod install"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "4.6.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@shopify/flash-list": "^1.6.4",
    "@thefunbots/react-native-pointer-interactions": "^1.1.0",
    "hermes-engine": "0.11.0",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-bootsplash": "5.5.3",
    "react-native-dotenv": "3.4.11",
    "react-native-gesture-handler": "2.16.0",
    "react-native-gradle-plugin": "0.71.19",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-localization": "2.3.2",
    "react-native-localize": "^3.1.0",
    "react-native-reanimated": "3.9.0-rc.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-svg": "15.2.0",
    "react-native-swift": "^1.2.3",
    "valtio": "^1.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/eslint-config": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@react-native/typescript-config": "0.74.81",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.6",
    "@types/react-native-dotenv": "^0.2.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.11",
    "prettier": "2.8.8",
    "reactotron-react-native": "^5.1.6",
    "ts-prune": "^0.10.3",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.7.0",
  "resolutions": {
    "react-native-haptic-feedback@^2.2.0": "patch:react-native-haptic-feedback@npm%3A2.2.0#./.yarn/patches/react-native-haptic-feedback-npm-2.2.0-13f039eb25.patch"
  }
}
