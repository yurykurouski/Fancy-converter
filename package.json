{
  "name": "fancy-converter",
  "version": "1.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug",
    "ios": "react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "start": "react-native start",
    "start:android": "adb reverse tcp:8081 tcp:8081 && react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type:check": "tsc --noemit --skipLibCheck",
    "upgrade": "yarn upgrade-interactive",
    "nuke": "rm -rf node_modules; rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock; yarn; yarn podinstall",
    "findDead": "ts-prune | grep -v '(used in module)'",
    "pods": "cd ios; pod install"
  },
