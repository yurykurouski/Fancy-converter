{
  "name": "fancy-converter",
  "version": "1.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug",
    "ios": "react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "start": "react-native start",
    "start:android": "adb reverse tcp:8081 tcp:8081 && react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type:check": "tsc --noemit --skipLibCheck",
    "upgrade": "yarn upgrade-interactive",
    "nuke": "rm -rf node_modules; rm -rf ios/build; rm -rf ios/Pods; rm -rf ios/Podfile.lock; yarn; yarn podinstall",
    "findDead": "ts-prune | grep -v '(used in module)'",
    "pods": "cd ios; pod install"
  },
  "dependencies": {
    "@expo/metro-runtime": "^3.1.3",
    "@gorhom/bottom-sheet": "5.0.0-alpha.6",
    "@react-native-async-storage/async-storage": "1.19.3",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-community/netinfo": "9.3.0",
    "@reduxjs/toolkit": "1.9.5",
    "@thefunbots/react-native-pointer-interactions": "^1.1.0",
    "expo": "^50.0.0",
    "expo-blur": "~12.9.1",
    "expo-localization": "~14.8.3",
    "hermes-engine": "0.11.0",
    "lodash.debounce": "4.0.8",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.4",
    "react-native-bootsplash": "5.3.0",
    "react-native-dotenv": "3.4.9",
    "react-native-gesture-handler": "2.15.0",
    "react-native-gradle-plugin": "0.70.3",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "4.7.2",
    "react-native-svg": "13.14.0",
    "react-native-swift": "^1.2.3",
    "react-native-web": "^0.19.10",
    "react-redux": "8.1.2",
    "recyclerlistview": "^4.2.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.2.6",
    "@types/react-native-dotenv": "^0.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "ts-prune": "^0.10.3",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.7.0"
}
